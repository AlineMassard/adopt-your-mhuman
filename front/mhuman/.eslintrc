{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb",
      "plugin:react-hooks/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
    }
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "linebreak-style": 0,
      "import/no-extraneous-dependencies": "warn",
      "no-unused-vars": "warn",
      "react/no-unescaped-entities": 0,
      "react/no-array-index-key": "warn",
      "max-len": ["warn", {"code": 150, "ignoreComments": true}]
  }
}